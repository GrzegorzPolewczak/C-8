<Window x:Class="ZadanieBLab8.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZadanieBLab8"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnWczytaj" Content="Wczytaj" HorizontalAlignment="Left" Height="99" Margin="31,45,0,0" VerticalAlignment="Top" Width="215" Click="btnWczytaj_Click"/>
        <ListBox x:Name="lbxListaStudentow" HorizontalAlignment="Left" Height="303" Margin="309,45,0,0" VerticalAlignment="Top" Width="425" d:ItemsSource="{d:SampleData ItemCount=5}"/>
        <Label x:Name="lbSredniaOcen" Content="Label" HorizontalAlignment="Left" Height="44" Margin="31,173,0,0" VerticalAlignment="Top" Width="242"/>
        <TextBox x:Name="txtImie" HorizontalAlignment="Left" Margin="32,222,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="241" Height="47"/>
        <TextBox x:Name="txtNazwisko" HorizontalAlignment="Left" Height="37" Margin="32,290,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="241"/>
        <TextBox x:Name="txtOcena" HorizontalAlignment="Left" Height="44" Margin="31,348,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="242"/>
        <Button x:Name="btnDodaj" Content="Dodaj" HorizontalAlignment="Left" Height="54" Margin="309,370,0,0" VerticalAlignment="Top" Width="213"/>

    </Grid>
</Window>
